<Window x:Class="PLWPF.GetKey"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="GetKey" Height="200" Width="300">
    <Grid Name="mainGrid">
        <Label Content="Please enter guest request key:" FontSize="16" HorizontalAlignment="Left" Margin="30,22,0,0" VerticalAlignment="Top" Height="37" Width="234"/>
        <TextBox Name="tbID" HorizontalAlignment="Left" FontSize="16" Height="36" Margin="30,64,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="234"/>
        <Button Content="Done" HorizontalAlignment="Left" Height="37" Margin="158,122,0,0" VerticalAlignment="Top" Width="74" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Height="37" Margin="44,122,0,0" VerticalAlignment="Top" Width="74"/>

    </Grid>
</Window>
