<Window x:Class="PLWPF.UploadOrderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="UploadOrderWindow" Height="450" Width="800">
    <Grid>
        <ComboBox Name="OrderstList"  VerticalAlignment="Stretch" SelectionChanged="cbRequestList_SelectionChanged" Text="{Binding Path=listOrders}" Margin="70,30,0,328.667" FontSize="36" Width="660"  HorizontalAlignment="Left" RenderTransformOrigin="0.5,0.5"/>
    </Grid>
</Window>
