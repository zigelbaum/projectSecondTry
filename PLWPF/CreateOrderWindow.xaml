<Window x:Class="PLWPF.CreateOrderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="CreateOrderWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="145*"/>
            <RowDefinition Height="276*"/>
        </Grid.RowDefinitions>
        <ComboBox Name="GuestRequestList"  VerticalAlignment="Stretch" Grid.Column="0" SelectionChanged="cbRequestList_SelectionChanged" Text="{Binding Path=matchRequests}" Margin="50,78.333,0,135.667" FontSize="36" Width="703"  HorizontalAlignment="Left" RenderTransformOrigin="0.5,0.5" Grid.Row="1">
            <ComboBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <RotateTransform/>
                    <TranslateTransform Y="1.209" X="0.598"/>
                </TransformGroup>
            </ComboBox.RenderTransform>
        </ComboBox>
        <TextBox Name="hostingUnitKey"  HorizontalAlignment="Left" Margin="144,37,0,0" TextWrapping="Wrap" Text="{Binding Path=myorder.HostingUnitKey}" VerticalAlignment="Top" Height="28" Width="114"/>
        <TextBox Name="hostingUnitKeyString"  HorizontalAlignment="Left" Margin="46,38,0,0" TextWrapping="Wrap" Text="Hosting Unit Key:" VerticalAlignment="Top" Height="28" Width="99"/>

        <TextBox Name="GuestRequestKey"  HorizontalAlignment="Center" Margin="645,39,34.333,0" TextWrapping="Wrap" Text="{Binding Path=OrderKey,Mode=OneWay}" VerticalAlignment="Top" Height="28" Width="114" Grid.RowSpan="1"/>
        <TextBox Name="GuestRequestKeyString"  HorizontalAlignment="Center" Margin="313,39,371.333,0" TextWrapping="Wrap" Text="Guest Request Key:" VerticalAlignment="Top" Height="28" Width="109" Grid.RowSpan="1"/>

        <TextBox Name="OrderKey"  HorizontalAlignment="Right" Margin="0,39,258.333,0" TextWrapping="Wrap" Text="{Binding Path=myorder.OrderKey}" VerticalAlignment="Top" Height="28" Width="114"/>
        <TextBox Name="OrderKeyString"  HorizontalAlignment="Right" Margin="0,39,147.333,0" TextWrapping="Wrap" Text="Order Key:" VerticalAlignment="Top" Height="28" Width="62" RenderTransformOrigin="1.405,0.447"/>

        <ComboBox Name="StatusOrder"  HorizontalAlignment="Left" Margin="108,0.333,0,0" Grid.Row="1"  Text="{Binding Path=status}" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="StatusOrderString"  HorizontalAlignment="Right" Margin="0,0.333,684.333,0" TextWrapping="Wrap" Text="status:" VerticalAlignment="Top" Height="22" Width="62" RenderTransformOrigin="1.405,0.447" Grid.Row="1"/>

        <TextBox Name="CreateDate"  HorizontalAlignment="Right" Margin="0,0.333,258.333,0" TextWrapping="Wrap" Text="{Binding Path=}" VerticalAlignment="Top" Height="22" Width="114" RenderTransformOrigin="1.405,0.447" Grid.Row="1"/>
        <TextBox Name="CreateDateString"  HorizontalAlignment="Right" Margin="0,0,402,0" TextWrapping="Wrap" Text="create date:" VerticalAlignment="Top" Height="22" Width="77" RenderTransformOrigin="1.405,0.447" Grid.Row="1"/>

        <TextBox Name="OrderDate"  HorizontalAlignment="Right" Margin="0,0.333,34.333,0" TextWrapping="Wrap" Text="{Binding Path=}" VerticalAlignment="Top" Height="22" Width="108" RenderTransformOrigin="1.405,0.447" Grid.Row="1"/>
        <TextBox Name="OrderDateString"  HorizontalAlignment="Right" Margin="0,144,139.333,0" TextWrapping="Wrap" Text="order date:" VerticalAlignment="Top" Height="23" Width="77" RenderTransformOrigin="1.405,0.447" Grid.RowSpan="2"/>

    </Grid>
</Window>
